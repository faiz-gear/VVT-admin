{
  "name": "vvt-admin",
  "private": true,
  "version": "1.0.0-beta.6",
  "scripts": {
    "dev": "vite",
    "build": "vite build && esno ./script/buildWithConfig.ts",
    "build:staging": "vite build --mode staging && esno ./script/buildWithConfig.ts ",
